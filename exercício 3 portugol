programa
{
	
	funcao inicio()
	{
		inteiro numero1 , numero2

		escreva("Digite o primeiro número : ")
		leia(numero1)
		limpa()

		escreva("Digite o segundo número : ")
		leia(numero2)
		limpa()

		se (numero1  > numero2 )
		{
			escreva(numero1," é maior que ",numero2)	
		}
		se (numero2  > numero1 )
		{
			escreva(numero2," é maior que ",numero1)	
		}
	}
}
